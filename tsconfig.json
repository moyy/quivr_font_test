{
  "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "sourceMap": true,
      "outDir": "dist",
      "resolveJsonModule": true,
      "lib": ["dom", "dom.iterable", "esnext"]
  },
  "include": [
      "src/**/*.ts"
  ],
  "exclude": [
      "node_modules"
  ]
}
